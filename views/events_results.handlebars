<div class="container">
    <div class="row">
        <div class="col s12">
            <h1 style="text-align: center">This Week</h1>
            <p class="subtitle text-center" >Press on an event to get reminders</p>
        </div>
    </div>

    {{#each events }}
    <div class="row">
        <div class="col s12 m7">
            <div class="card small" id={{title}} data-id="{{title}}">
                <div class="card-image waves-effect waves-block waves-light">
                    <img src="{{admin.imageUrl}}">
                    <div class="overlay"></div>
                </div>
                <div class="card-content">
                    <span class="card-title center-align" >{{title}}</span>
                    <h5 class="center-align org-title">{{admin.name}}</h5>
                    <h6 class="center-align "><i class="tiny material-icons">access_time</i> {{timePretty scheduled_date}}</h6>
                </div>
            </div>
        </div>
    </div>
    {{/each}}

    <div class="row">
        <div class="col s12">
            <div class="text-center">
                <a class="waves-effect waves-light btn-large btn-full-width submit">Submit</a>
            </div>
        </div>
    </div>
    <div class="scroll-btn">
        <a class="waves-effect waves-light btn-large btn-full-width orange darken-3" >Press to see more!</a>
    </div>
</div>




<script type="text/javascript" src="/js/activity_events.js"></script>
