
<div id="{{id}}">
    <div class="row">
        {{> logosection}}
        <div class="col s12">
            <div class="from-them">
                <p class="question">{{question}}</p>
            </div>
        </div>
    </div>
    <div class="submit-section">
        <div class="col s6">
            <a class="waves-effect waves-light btn-large btn-full-width text-center resp-option" data-type="continue" data-value="yes" data-qid="{{id}}">Yes</a>
        </div>
        <div class="col s6">
            <a class="waves-effect waves-light btn-large btn-full-width text-center resp-option" data-type="continue" data-value="no" data-qid="{{id}}">No</a>
        </div>
    </div>
</div>
