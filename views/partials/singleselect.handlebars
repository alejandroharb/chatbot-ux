<div id="{{sequence}}">
    <div class="row">
        {{> logosection}}
        <div class="col s12">
            <div class="from-them">
                <p class="question">{{question}}</p>
            </div>
        </div>
    </div>
    <div class="row">
        {{#each options}}
        <div class="col s12 m6">
            <a class="waves-effect waves-light btn-large btn-full-width btn-list resp-option" data-qid="{{../sequence}}" data-value="{{this}}">{{this}}</a>
        </div>
        {{/each}}
    </div>
    <div class="submit-section"></div>
</div>
