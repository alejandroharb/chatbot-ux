<div class="container mobile-container">
    <div class="row">
        <div class="col s12">
          <img class="responsive-img" src="/images/logo-web-transparent.png"/>
        </div>
    </div>
    <div class="row">
        <div class="col s12">
          <h4 class="text-center">{{event.title}}</h4>
          <h5 class="text-center">{{timePretty event.scheduled_date}}</h5>
          <h5 class="text-center">Status: {{event.conference.status}}</h5>
        </div>
    </div>
    <div class="row">
        <div class="col s12">
            <div class="text-center">
                <a class="waves-effect waves-light round-btn btn blue darken-3 {{conferenceExpiredDisable event.conference.status}}" id="call-submit" data-phone="{{phone}}" data-eventId="{{event._id}}">
                    <p>Press to Join Group Call</p>
                </a>
            </div>
        </div>
    </div>
</div>




<script type="text/javascript" src="/js/group_call_in.js"></script>
